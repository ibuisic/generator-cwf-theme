 *
 * @ingroup themeable
 */
#}
{% extends "form--user-form.html.twig" %}
{% block form_header %}
  {{ 'Sign up'|t }}
{% endblock %}
{% block form_footer %}
  <div class="text-center">
    <div class="btn-group">
      <a class="btn btn-primary" href="{{ path('user.login') }}">{{ 'Login'|t }}</a>
      <a class="btn btn-danger"  href="{{ path('user.pass') }}">{{ 'Forgot password?'|t }}</a>
    </div>
  </div>
{% endblock %}
